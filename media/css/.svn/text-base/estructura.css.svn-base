/* @group menu orden */

#orden
{
border-bottom: 1px solid #a5b57f;
border-top: 1px solid #a5b57f;
padding-left: 180px;
background-color: #a5b57f;
padding-bottom: 3px;
padding-top: 3px;
}

#orden a, #orden a:link, #orden a:visited
{
border: 1px solid #a5e367;
padding: 1px;
padding-left: 0.5em;
padding-right: 0.5em;
color: #000000;
font-weight: bold;
text-decoration: none;
}

#orden a:hover, #orden a:active, #orden a:focus
{
border: 1px solid #000000;
padding: 1px;
padding-left: 0.5em;
padding-right: 0.5em;
text-decoration: none;
}

#orden li
{
padding-right: 1px;
display: inline;
font-size: 0.8em;
}

#orden ul
{

}
#orden .active a { background-color: #FFCC66; }

/* @end */
/* @group menu */
#navcontainer { width: 100px; }

#navcontainer ul
{
	margin-left: 0;
	padding-left: 0;
	list-style-type: none;
	font-family: Arial, Helvetica, sans-serif;
}

#navcontainer ul li a
{
	display: block;
	padding: 3px;
	width: 160px;
	background-color: #036;
	border-bottom: 1px solid #eee;
}
#navcontainer ul li a.current
{
	background-color: #cc0033;
}

#navcontainer ul li a:link, #navcontainer ul li a:visited
{
	color: #EEE;
	text-decoration: none;
}

#navcontainer ul li a:hover
{
	background-color: #369;
	color: #fff;
}

/* @end menu */

/* @group Border */


#actualizaciones ul {
    text-indent: 5px;
    border-bottom-style: solid;
    border-bottom-width: 1px;
    border-bottom-color: #006699;
    padding-bottom: 5px;
}
#actualizaciones ul>ul {
    text-indent: 10px;
}
#menudatos{
    margin: 10px 0;
}

/* @end */

/* @group Paginaci√≥n */

.pagination {
    display:block;
    color:teal;
    text-align:center;
    padding: 5px 0px;
}
.pagination strong{
	border: 1px solid silver;
    padding: 1px 4px;
    font-weight: bold;
    color:white;
    background-color:teal;
}
.pagination a {
    border: 1px solid silver;
    color:#cc0033;
    padding: 1px 4px;
    font-weight: bold;
    text-decoration: none;
}
.pagination a:hover{
    color:white;
    background-color:#cc0033;
}

/* @end */

/* @group Extras */

.extras{
    font-size: .8em;
}
.rubro {
	font-weight: bold;
}
.rubros {
    font-size: .8em;
}
.rubros a {
	display: block;
}
.rubros li:hover{
	background-color: #c6e3e5;
	color: black;
}
.rubros li .principal{
	background-color: teal;
	font-weight: bold;
	color: white;
}

/* @end */

/* @group Tool tip */
 
div.tooltip {
	background-color:#000;
	border:1px solid #fff;
	padding:10px 15px;
	width:200px;
	display:none;
	color:#fff;
	text-align:left;
	font-size:12px;

	
	-moz-box-shadow:0 0 10px #000;
	-webkit-box-shadow:0 0 10px #000;
}


#tooltip {
	width:150px;
	background-color:#3d7483;
	padding:5px;
	font-size:11px;
	z-index: 1000;
}


/* @end */