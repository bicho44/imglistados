/* @group Base */


html {
	background-color: #e4e4e4;
}
strong{
	font-weight:bold;
}

body{
	font-family: Arial, Verdana, sans-serif;
}
h1, h2, h3, h4{
	font-weight: bold;
}

h1{
	font-size: 123.1%;
}

h2{
	font-size: 116%;
}

h3 {
	font-size: 108%;
}


/* @end */

/* @group Estructura */
#hd {
	height: 85px;
	color: white;
}

#doc4 {
	background-color: #415a81;
	border: 1px solid #787878;
}

#yui-main, .navigator {
	background-color: #d7eff3;
}

.data {
	background-color: white;
	border-right: 1px solid #bfd5d9;
	padding: 10px;
}

#buscador {
	color: white;
	background: #f90;
	padding: 5px;
}

#ft {
	color: white;
	text-align: center;
	padding: 5px;
	font-size: 77%;
}

#ft a{
	font-weight:bold;
	color:white;
	text-decoration: none ;
}

#ft a:hover{
	color: gray;
}
.datos{
	background-color: #e0e9df;
	border: 1px solid #96a86d;
	margin-bottom: 5px;
	padding: 5px;
}
.spacer{
	padding-right: 5px;
	padding-left: 5px;
}

/* @end */

/* @group Dashboard */

.mensajes {
	margin-bottom: 5px;
	font-weight: normal;
}

table.mensajes  {
	width: 100%;
	padding: 2px;
}

table.mensajes thead, table.mensajes tfoot, table.mensajes tbody tr td {
	padding: 4px;
}

table.mensajes thead tr th, table.mensajes tfoot tr td {
	color: white;
	background-color:#415a81;
	text-align: center;
	font-weight: bold;
}

/* @end */

/* @group Importancia */
.odd{
	background-color: white;
}
.even{
	background-color: #e1f5f5;
}

.destacado{
	background-color: #ead9f6;
	border-bottom: 2px solid white;
}
.order0 {
	background-color: #ff7372;
	color: white;
}

.order0 a:link, .order0 a:visited, .order0 a:active {
	color: white;
	text-decoration: none;
}

.order1 {
	background-color: #ffb759;
	color: black;
}
.order1 a:link, .order1 a:visited, .order1 a:active {
	text-decoration: none;
}
.order2 {
	background-color: #fbff6b;
	color: black;
}

.order3 {
	background-color: #959a43;
	color: white;
}
.order3 a:link, .order3 a:visited, .order3 a:active {
	color: white;
	text-decoration: none;
}
.order4 {
	background-color: #3fae5c;
	color: white;
}
.order4 a:link, .order4 a:visited, .order4 a:active {
	color: white;
	text-decoration: none;
}
.odd a, .even a {
	color: black;
	text-decoration: none;
}
/* @end */
/* @group Formularios */
.border {
    background-color: #f7fff6;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
	padding: 10px;
	border: 1px solid #96a86d;
	margin: 0 0 5px 0;
}
.border h3 {
	margin-bottom: 10px;
}
form.cmxform fieldset {
	border: 1px solid silver;
	margin-bottom: 10px;
}
form.cmxform fieldset legend {
	font-size: 14px;
	font-weight: bold;
	text-indent: 5px;
}
form.cmxform label {
	padding-right: 2px;
	padding-left: 5px;
	font-weight: bold;
	display: inline-block;
	line-height: 1.8;
	vertical-align: top;
}
form.cmxform input{
	background-color: #eeebff;
	border-style: solid;
	border-color: #d4d8ff;
	border-width: 1px;
}

